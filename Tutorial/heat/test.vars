// non-reacting sub-sonic inviscid cylinder simulation using preconditioning
{
boundary_conditions:<
 BC_1=specified(Twall=500K), // Blob surface
 BC_2=specified(Twall=300K),
 BC_3=adiabatic, BC_4=adiabatic     // opposing slice faces
>

deltaT: 10 // 10 second time-step

T_initial:300 // initial conditions

// Properties for copper
Density: 8960
Cp: 0.383
conductivity: 394

stop_iter: 10
plot_freq: 5
plot_modulo:100
}
