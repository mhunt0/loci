<?xml version='1.0' encoding='ISO-8859-1'?>
<boundary>
  
  <!--for each option, specify its element, it can be 'float', -->
  <!--'int','vector', 'string' or 'selection'. if an option has -->
  <!--unit, specify it as a string list separated by ','-->
  <options>
    <temperature unit="K,C,F,R" element="float" />
    <totalTemperature unit="K,C,F,R" element="float" />
    <pressure unit="atm,Pa,psi,lbf/in/in,lbf/ft/ft,slug/ft/ft" element="float" />
    <totalPressure unit="atm,Pa,psi,lbf/in/in,lbf/ft/ft,slug/ft/ft" element="float" />
    <mass_flow unit="kg/s,lbm/s" element="float" />
    <mass_flux unit="kg/m/m/s" element="float" />
    <density unit="kg/m/m/m,g/cm/cm/cm,lbm/in/in/in,lbm/ft/ft/ft,slugs/ft/ft/ft" element="float" />
    <time unit="s,m,h,ms,ns" element="float" />
    <velocity unit="m/s,km/h,miles/hour,ft/s" element="vector" />
    <speed unit="rpm,rps" element="float" />
    <Mach_number element="float" />
    <name element="string" />
    <mass_fraction element="string" />
    <vector element="vector" />
    <angle unit="deg,rad" element="float" />
    </options>

  <!-- specify the groups of options, for each group, the element can-->
  <!-- be 'all', '2of3' or 'stack' -->
 
  <!-- all: all the children need to be specified, so children are not-->
  <!-- checkable -->
 
  <!-- '2of3': there are three checkable children, only two of them can be checked-->
  
<!-- 'stack': the user can clicked on one of the checkable children,-->
  <!--  the input interface for the checked child will be displayed. --> 
  
  <!--  the text node of element node  is the variable name. For example, -->
  <!-- temperature can be T, T0, or Twall, depend on what kind of -->
  <!-- boundary it is. -->

  <!-- the attribute 'title' specifys what's displayed as the title of the -->
  <!--group on the interface -->

  <groups>
    <prhotGroup title="please check 2 out of 3" element="2of3">
      <pressure name="p"></pressure>
      <temperature name="T"></temperature>
      <density name="rho"></density>
    </prhotGroup>
    <prhot0Group title="please check 2 out of 3" element="2of3">
      <pressure name="p0"></pressure>
      <density name="rho0"></density>
      <temperature name="T0"></temperature>
    </prhot0Group>
    <prhotwallGroup title="please check 2 out of 3" element="2of3">
      <pressure name="pwall"></pressure>
      <density name="rhowall"></density>
      <temperature name="Twall"></temperature>
    </prhotwallGroup>
    <swirlGroup   element="all" >
      <vector name="swirlAxis" title="swirl axis"></vector>
      <vector name="swirlCenter" title="swirl center"></vector>
      <angle name="swirlAngle" title="swirl angle"></angle>
    </swirlGroup>
    <rotGroup   element="all">
      <vector name="torAxis"></vector>
      <vector name="rotCenter"></vector>
      <speed name="rotSpeed"></speed>
    </rotGroup>
    <swirlflowGroup title="swirlflow" element="stack" >
      <swirlGroup title="fixed swirling flow angle" />
      <rotGroup  title="rigid body rotation"  />
    </swirlflowGroup>
    <turbulenceGroup title="turbulence" element="stack" >
      <swirlGroup title="fixed swirling flow angle"  />
      <rotGroup  title="rigid body rotation"  />
    </turbulenceGroup>
    <velocityGroup title="select one" element="stack" >
      <velocity title="velocity" name="u"></velocity>
      <Mach_number title="Mach Number" name="M" ></Mach_number>
    </velocityGroup>
    <massGroup title="select one" element="stack" >
      <mass_flow title="mass flow" name="mdot"></mass_flow>
      <mass_flux title="mass flux" name="massFlux"></mass_flux>
    </massGroup>
    <twallGroup title="select one" element="stack" >
      <temperature title="temperature" name="Twall"></temperature>
       <flags element="selection">
         <adiabatic />
       </flags>
    </twallGroup>
  </groups>

 <!-- specify the interface here. It includes groups and options. -->
  <!-- The text of nodes  are the variable names appear in  output. -->
  <!-- if the page need advanced features, specify attribute 'advanced'. If the -->
   <!-- if atribute 'checked' is specified, this option/group is optional  -->
   <!--  Attribute "title" specify the text displayed on interface-->
  <boundaryPage element="boundaryWondow" title="set up boundary
  conditions" label="please specify boundary type:" >
    <notset/>
    <inflow toolTip="A non-reflecting inflow boundary, can be&#xa;   supersonic or subsonic inflow." >
      <prhotGroup/>
      <velocityGroup/>
      <mass_fraction checked="0" element="string" name="mixture"></mass_fraction>
      <advanced>
        <swirlflowGroup/>
	<turbulenceGroup/>
       </advanced>  
      <flags  element="selection" >
        <normal/>
	<equilibrium/>
      </flags>
    </inflow>
    <supersonicInflow  toolTip="Used for supersonic inflow&#xa;    conditions.  Can also be used as a Dirichlet boundary condition " >
      <prhotGroup  />
      <velocityGroup  />
      <mass_fraction checked="0" ></mass_fraction>
      <advanced>
        <swirlflowGroup/>
	<turbulenceGroup/>
       </advanced>       
       <flags element="selection" >
        <normal/>
	<equilibrium/>
       </flags>
         
    </supersonicInflow>
    <isentropicInflow  toolTip="Used for subsonic inflow where total&#xa;    conditions are specified and held." >
      <pressure title="total pressure" name="P0"></pressure>
      <temperature title="total temperature" name="T0"></temperature>
      <mass_fraction checked="0" name="mixture"></mass_fraction>
     
       <advanced>
        <swirlflowGroup/>
	<turbulenceGroup/>
       </advanced>  
      <flags  element="selection" >
        <normal/>
      </flags> 
    </isentropicInflow>
    <fixedMass  toolTip="Used for subsonic inflow where the mass flow&#xa;     rate is prescribed." >
      <massGroup  />
      <temperature title="total temperature" name="T0"></temperature>
     
       <advanced>
        <swirlflowGroup/>
	<turbulenceGroup/>
       </advanced> 
       <flags  element="selection" >
        <normal/>
      </flags>  
    </fixedMass>
    <farfield  toolTip="A non-reflecting inflow and outflow&#xa;    condition.  This is often used to specify the farfield boundary on&#xa;    an external flow problem " >
      <prhotGroup  />
      <velocityGroup  />
      <mass_fraction checked="0" name="mixture"></mass_fraction>
      
       <advanced>
        <swirlflowGroup/>
	<turbulenceGroup/>
       </advanced>  
       <flags element="selection" >
        <normal/>
	<equilibrium/>
       </flags> 
    </farfield>
    <outflow toolTip="A pressure imposed subsonic outflow boundary." >
      <pressure name="p"></pressure>
    </outflow>
    <supersonicOutflow toolTip="Boundary for outflow boundaries&#xa;   where the flow is entirely supersonic." />
    <extrapolate toolTip="Used for supersonic outflows or outflows&#xa;   where zero normal gradients are expected." />
    <symmetry toolTip="Used for symmetry planes in 2-D simulations" />
    <impermeable toolTip="A slip wall boundary condition.  This&#xa;   should only be used for structured grid." />
    <reflecting toolTip="A general purpose sliding wall boundary" />
    <viscousWall toolTip="A non-slip viscous wall." >
      <twallGroup  />
      <vector checked="0" name="uwall"></vector>
    </viscousWall>
    <wallLaw toolTip="A turbulent viscous wall where the law-of-the-wall model is implemented." >
      <twallGroup  />
      <vector checked="0" name="uwall"></vector>
    </wallLaw>
    <periodic toolTip=" " >
      <name name="name"></name>
      <vector name="translate"></vector>
      <vector name="center"></vector>
      <vector name="vector"></vector>
      <angle name="rotate"></angle>
    </periodic>
    <interface toolTip=" " />
  </boundaryPage>


  <!-- This part is the output of program-->
  <boundary_conditions >
  </boundary_conditions>
</boundary>
