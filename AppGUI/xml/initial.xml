<?xml version='1.0' encoding='ISO-8859-1'?>
<initial>
  

  <!--for each option, specify its element, it can be 'float', -->
  <!--'int', 'string','vector' or 'selection', if an option has -->
  <!--unit, specify it  a string list separated by ','-->
  <options>
    <temperature unit="K,C,F,R" element="float" />
    <pressure unit="atm,Pa,psi,lbf/in/in,lbf/ft/ft,slug/ft/ft" element="float" />
    <density unit="kg/m/m/m, g/cm/cm/cm,lbm/in/in/in,lbm/ft/ft/ft,slugs/ft/ft/ft" element="float" />
    <velocity unit="m/s,km/h,miles/hour,ft/s" element="vector" />
    <Mach_number element="float" />
    <mass_fraction element="string" />
    <kinetic_energy unit="mm/s/s" element="float" />
    <dissipation_rate unit="s-1" element="float" />
    <time unit="s,m,h,ms,ns" element="float" />
  </options>

   <!-- specify the groups of options, for each group, the element can-->
  <!-- be 'all', '2of3' or 'stack' -->
 
  <!-- all: all the children need to be specified, so children are not-->
  <!-- checkable -->
 
  <!-- '2of3': there are three checkable children, only two of them can be checked-->
  <!-- 'stack': the user can clicked on one of the checkable children,-->
  <!--  the input interface for the checked child will be displayed. --> 
  
  <!--  the text node of element node  is the variable name. For example, -->
  <!-- temperature can be T, T0, or Twall, depend on what kind of -->
  <!-- boundary it is. -->

   <!-- the attribute 'title' specifys what's displayed as the title of the -->
   <!--group on the interface -->

  <groups>
    <prhotGroup title="please check 2 out of 3" element="2of3" >
      <pressure>p</pressure>
      <temperature>T</temperature>
      <density>rho</density>
    </prhotGroup>
    <velocityGroup title="select one" element="stack" toolTip="i'm velocityGroup" >
      <velocity title="velocity" name="u" toolTip="I'm velocity" ></velocity>
      <Mach_number title="Mach Number" name="M" toolTip="I;m M"></Mach_number>
    </velocityGroup>
  </groups>
  
  <!-- specify the interface here. It includes groups and options. -->
  <!-- The text of nodes  are the variable names appear in  output. -->
   <!-- if the page need advanced features, specify attribute 'advanced'. If the -->
   <!-- if atribute 'checked' is specified, this option/group is optional  -->
   <!--  Attribute "title" specify the text displayed on interface-->
  <initialPage current="0" element="initialWindow" >
    <initialConditions current="" element="OptionPage" whatsThis="set up initial conditions" >
      <prhotGroup  />
      <velocityGroup  />
      <mass_fraction title="mass fraction" checked="0" >mixture</mass_fraction>
      <kinetic_energy title="turbulence kinetic energy" checked="0" >k</kinetic_energy>
      <dissipation_rate title="specific dissipation rate" checked="0" >w</dissipation_rate>
      <flags element="selection">      
         <equilibrium />
      </flags> 
    </initialConditions>
    <qlqr element="all OptionPage" whatsThis="set up ql and qr" >
      <ql current="" >
        <prhotGroup />
        <velocityGroup  />
        <mass_fraction title="mass fraction" checked="0" >mixture</mass_fraction>
        <kinetic_energy title="turbulence kinetic energy" checked="0" >k</kinetic_energy>
        <dissipation_rate title="specific dissipation rate" checked="0" >w</dissipation_rate>
	<flags element="selection" >
	 <equilibrium/>
	</flags>        
      </ql>
      <qr current="" >
        <prhotGroup  />
        <velocityGroup  />
        <mass_fraction title="mass fraction" checked="0" >mixture</mass_fraction>
        <kinetic_energy title="turbulence kinetic energy" checked="0" >k</kinetic_energy>
        <dissipation_rate title="specific dissipation rate" checked="0" >w</dissipation_rate>
        <flags element="selection" >
	 <equilibrium/>
        </flags>   
      </qr>
    </qlqr>
    <interpolateInitialConditions current="" action="find file" exp="put*" element="string" whatsThis="specify a binary initial&#xa; condition file." checked="0" />
  </initialPage>
</initial>
